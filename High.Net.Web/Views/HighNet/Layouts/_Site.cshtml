@using High.Net.Core

@{
    Layout = "~/Views/Shared/Layouts/_root.cshtml";
}

@model IPageViewModel<BasePageData>

@section Header
{
    <link type="image/ico" href="~/Static/global/favicon/favicon.ico" rel="icon">
    @Html.IncludeVersionedCss("/dist/css/styles.min.css")
    @RenderSection("SiteHeader", required: false)

    <!--[if lt IE 9]>
        <script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="//oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
}

<div class="highnet">
    @Html.Partial("~/Views/HighNet/Shared/Header.cshtml", Model)
    @Html.Partial("~/Views/HighNet/Shared/Navigation.cshtml", Model)
    @RenderBody()
    <div class="main">
        @Html.Partial("~/Views/HighNet/Shared/Footer.cshtml", Model)
    </div>
</div>

@section Footer
{
    @*<script src="//www.youtube.com/player_api"></script>*@
    @Html.IncludeVersionedJs("/dist/scripts/scripts.min.js")
    @Scripts.Render("~/bundles/highnet-resuablecomponent-js")
    @RenderSection("SiteFooter", required: false)
    @*<script src="https://www.google.com/recaptcha/api.js?onload=initRecaptchaElements&render=explicit&hl={0}" async defer></script>*@
}


