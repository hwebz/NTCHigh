@using High.Net.Core
@using High.Net.Models.HighNet.Pages
@using High.Net.Web.ViewModels.Leadership

@{
    Layout = "~/Views/Shared/Layouts/_root.cshtml";
}

@model IPageViewModel<BasePageData>


@section Header
{
    <link type="image/ico" href="~/Static/global/favicon/favicon.ico" rel="icon">
    @Html.IncludeVersionedCss("/dist/Leadership/css/styles.min.css")
    @RenderSection("SiteHeader", required: false)

    <!--[if lt IE 9]>
        <script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="//oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
}

@Html.Partial("~/Views/Leadership/Shared/BannerStyle.cshtml", Model.CurrentPage)


@Html.Partial("~/Views/Leadership/Shared/Navigation.cshtml", Model)

@RenderBody()

@Html.Partial("~/Views/Leadership/Shared/Footer.cshtml", Model)

@section Footer
{
    @Html.IncludeVersionedJs("/dist/Leadership/scripts/scripts.js")
    @RenderSection("SiteFooter", required: false)
}



