@using EPiServer.Core
@using High.Net.Core
@using High.Net.Models.Rollover.Blocks

@model MainBlock

<div class="welcome asset-services">
    <h2 class="title">@Html.PropertyFor(x => x.Heading)</h2>
    @*<hr class="blue-border" />*@
