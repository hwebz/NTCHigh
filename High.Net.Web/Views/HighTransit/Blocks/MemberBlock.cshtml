@using EPiServer.Core
@using EPiServer.Web.Mvc.Html
@using High.Net.Models.HighTransit.Blocks
@using ImageVault.EPiServer
@model MemberBlock
<div class="row">
    <div class="col-md-2 col-sm-2">
        <div class="member-img">
            @Html.PropertyFor(x => Model.MemberImage, new { PropertySettings = new PropertyMediaSettings { Width = 163, Height = 87}, CssClass = "img-responsive" })
        </div>
    </div>
    <div class="col-sm-10 col-md-10">
        <div class="inner-para">
            <p>
                @Html.PropertyFor(x => x.Description)
            </p>
        </div>
    </div>
</div>
<div class="spacer"></div>

